{% extends 'base.html.twig' %}
{% block title %}Products{% endblock %}
{% block main %}
    <div id="products">
        {% if products is empty %}
            <p> Aucun produit disponible en ce moment :(</p>
        {% endif %}
        {% for product in products %}
            {{ include('product/product.html.twig', {'product' : product}) }}
        {% endfor %}
{% endblock %}